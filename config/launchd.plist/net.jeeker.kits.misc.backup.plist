<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Label</key>
        <string>net.jeeker.kits.misc.backup</string>
        <key>ProgramArguments</key>
        <array>
            <string>/bin/bash</string>
            <string>-c</string>
            <string>. ~/.kits_task; task_backup</string>
        </array>
        <!-- 每天 21:00 -->
        <key>StartCalendarInterval</key>
        <dict>
            <key>Hour</key>
            <integer>21</integer>
            <key>Minute</key>
            <integer>0</integer>
        </dict>
        <!-- 调试输出 -->
        <key>StandardOutPath</key>
        <string>/Users/JinnLynn/Library/Logs/net.jeeker.kits/launchd.log</string>
        <key>StandardErrorPath</key>
        <string>/Users/JinnLynn/Library/Logs/net.jeeker.kits/launchd.log</string>
        <key>Debug</key>
        <true/>
    </dict>
</plist>